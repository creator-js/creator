{
  "name": "creator-js-cli",
  "version": "1.0.2",
  "description": "Generate whatever you want",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "jest --coverage",
    "build": "rimraf dist && webpack --config webpack.config.js && cp cli.js dist && tsc",
    "g1": "node ./dist/main.js",
    "g": "node_modules/.bin/g",
    "release": "release-it --no-git.requireCleanWorkingDir",
    "prepare": "husky install"
  },
  "bin": {
    "g": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "generate",
    "redux",
    "components"
  ],
  "author": "Michael Kutateladze",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creator-js/creator"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creator-js/creator/issues"
  },
  "homepage": "https://github.com/creator-js/creator#readme",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.4.1",
    "@types/react-redux": "^7.1.23",
    "@types/redux-actions": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^8.2.5",
    "eslint": "8.28.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.2",
    "inquirer": "^7.3.3",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "creator-js-cli": "1.0.1",
    "release-it": "^14.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "4.9.3",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "resolutions": {
    "eslint": "8.28.0"
  },
  "dependencies": {
    "rxjs": "^7.5.5"
  },
  "release-it": {
    "github": {
      "release": false
    }
  }
}
